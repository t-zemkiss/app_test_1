# Database Configuration - MySQL
DB_HOST=localhost
DB_USER=your_mysql_user # e.g., root
DB_PASSWORD=your_mysql_password
DB_NAME=ai_image_app_db
DB_PORT=3306

# Server Configuration
PORT=5000

# Hugging Face API (Simulated)
HF_API_KEY=your_hugging_face_api_key_here
DEFAULT_HF_MODEL_ID=simulated_model_v1

# JWT Authentication
JWT_SECRET=a_very_strong_and_unique_secret_for_jwt_please_change_this
JWT_EXPIRES_IN=7d

# Application Specific
INFO_VARIABLE_CREDITO_COSTE=50
DEFAULT_USER_CREDITS=100

# Google OAuth (Optional)
# Register your application at https://console.cloud.google.com/ to get these
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Node Environment
NODE_ENV=development # production or development
